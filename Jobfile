# Add Jobs below in the format of TOML
# Note that cmd should be given with absolute path

# Example Jobs

[[job]]
name = 'Job 1'
cmd: '/usr/bin/touch /tmp/1'
schedule: '0 0/1 * * * *'

[[job]]
name = 'Job 2'
cmd: '/usr/bin/touch /tmp/2'
schedule: '0 0/2 * * * *'
